{
  "name": "ideaclinic-mobile",
  "version": "1.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client --reset-cache",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.0",
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@novu/react-native": "^2.6.8",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^7.0.3",
    "@rn-primitives/alert-dialog": "^1.1.0",
    "@rn-primitives/avatar": "^1.1.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/popover": "^1.1.0",
    "@rn-primitives/select": "^1.1.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/switch": "^1.1.0",
    "@rn-primitives/tabs": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@roninoss/icons": "^0.0.4",
    "@shopify/flash-list": "1.7.3",
    "@supabase/supabase-js": "^2.38.4",
    "base64-arraybuffer": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~52.0.38",
    "expo-application": "~6.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-image": "~2.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-insights": "~0.8.2",
    "expo-linking": "~7.0.5",
    "expo-navigation-bar": "~4.0.8",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-store-review": "~8.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "moment": "^2.30.1",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-uitextview": "^1.4.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "3",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@microsoft/eslint-formatter-sarif": "3.1.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "ajv": "^8.12.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "expo-atlas": "^0.4.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@rn-primitives/alert-dialog",
          "@rn-primitives/avatar",
          "@rn-primitives/label",
          "@rn-primitives/select",
          "@rn-primitives/separator",
          "@rn-primitives/slot",
          "@rn-primitives/switch",
          "@rn-primitives/tabs",
          "@rn-primitives/types",
          "@rn-primitives/popover",
          "@roninoss/icons",
          "@novu/react-native",
          "base64-arraybuffer",
          "class-variance-authority",
          "cloudinary-react-native",
          "moment",
          "react-native-uuid",
          "tailwindcss"
        ]
      }
    }
  }
